<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="net.mjrz.fm.entity.beans.AccountCategory" table="TB_ACCOUNTCATEGORIES">
		<id name="categoryId" column="accountId" type="java.lang.Long">
		</id>
		<property name="uid" type="long" />
		<property name="parentCategoryId" type="long" />
		<property name="categoryName" type="java.lang.String">
			<column name="categoryName" sql-type="varchar(256)"/>
		</property>
		<property name="createdDate" type="java.util.Date" />
	</class>
</hibernate-mapping>