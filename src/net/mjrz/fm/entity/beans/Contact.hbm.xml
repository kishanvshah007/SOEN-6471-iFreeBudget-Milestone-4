<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="net.mjrz.fm.entity.beans.Contact" table="TB_CONTACT">
		<id name="id" column="id" type="long">
		</id>
		<property name="userId" type="long"/>
		<property name="fullName" type="java.lang.String"/>
		<property name="jobTitle" type="java.lang.String"/>
		<property name="company" type="java.lang.String"/>
		<property name="busPhone" type="java.lang.String"/> 
		<property name="homePhone" type="java.lang.String"/> 
		<property name="busFax" type="java.lang.String"/> 
		<property name="mobPhone" type="java.lang.String"/> 
		<property name="street" type="java.lang.String"/> 
		<property name="city" type="java.lang.String"/>
		<property name="state" type="java.lang.String"/>
		<property name="zip" type="java.lang.String"/>
		<property name="country" type="java.lang.String"/>
		<property name="email" type="java.lang.String"/>
		<property name="altEmail" type="java.lang.String"/>
		<property name="webAddr" type="java.lang.String"/>		
		<property name="imAddr" type="java.lang.String"/>		
		<property name="notes" type="java.lang.String"/>		
		<property name="createDate" type="java.util.Date"/>
	</class>
</hibernate-mapping>